          SQLTask:SqlStatementSource="DELETE FROM stg.Users&#xA;WHERE StgID NOT IN (&#xA;&#x9;SELECT StgID&#xA;&#x9;FROM (&#xA;&#x9;&#x9;SELECT&#xA;&#x9;&#x9;&#x9;StgID,&#xA;&#x9;&#x9;&#x9;ROW_NUMBER() OVER (PARTITION BY UserID ORDER BY&#xA;&#x9;&#x9;&#x9;&#x9;GREATEST(COALESCE(RegistrationDate, '1900-01-01'), COALESCE(LastLoginDate, '1900-01-01')) DESC) AS rn&#xA;&#x9;&#x9;&#x9;FROM stg.Users&#xA;&#x9;&#x9;) AS MostRecentUser&#xA;&#x9;WHERE rn = 1&#xA;);&#xA;&#xA;SELECT COUNT(*) AS RemoveDupeUserIDRowCount FROM stg.Users;"
          SQLTask:ResultType="ResultSetType_SingleRow" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
          <SQLTask:ResultBinding
            SQLTask:ResultName="RemoveDupeUserIDRowCount"
